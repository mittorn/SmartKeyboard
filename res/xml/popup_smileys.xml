<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="15%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row android:rowEdgeFlags="top">
        <Key android:keyLabel="%smiley_00" android:keyOutputText="%smiley_00 " android:keyEdgeFlags="left" />
        <Key android:keyLabel="%smiley_01" android:keyOutputText="%smiley_01 "/>
        <Key android:keyLabel="%smiley_02" android:keyOutputText="%smiley_02 "/>
        <Key android:keyLabel="%smiley_03" android:keyOutputText="%smiley_03 "/>
        <Key android:keyLabel="%smiley_04" android:keyOutputText="%smiley_04 " android:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key android:keyLabel="%smiley_05" android:keyOutputText="%smiley_05 " android:keyEdgeFlags="left" />
        <Key android:keyLabel="%smiley_06" android:keyOutputText="%smiley_06 "/>
        <Key android:keyLabel="%smiley_07" android:keyOutputText="%smiley_07 "/>
        <Key android:keyLabel="%smiley_08" android:keyOutputText="%smiley_08 "/>
        <Key android:keyLabel="%smiley_09" android:keyOutputText="%smiley_09 " android:keyEdgeFlags="right" />
    </Row>
    <Row android:rowEdgeFlags="bottom">
        <Key android:keyLabel="%smiley_10" android:keyOutputText="%smiley_10 " android:keyEdgeFlags="left" />
        <Key android:keyLabel="%smiley_11" android:keyOutputText="%smiley_11 "/>
        <Key android:keyLabel="%smiley_12" android:keyOutputText="%smiley_12 "/>
        <Key android:keyLabel="%smiley_13" android:keyOutputText="%smiley_13 "/>
        <Key android:keyLabel="%smiley_14" android:keyOutputText="%smiley_14 " android:keyEdgeFlags="right" />
    </Row>
</Keyboard>
