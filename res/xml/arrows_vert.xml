<Rows xmlns:android="http://schemas.android.com/apk/res/android">
	<Row android:keyWidth="10%p">
		<Key android:codes="-104" android:keyIcon="@drawable/up_arrow"
			android:iconPreview="@drawable/up_arrow" android:keyEdgeFlags="right"
			iconID="@+id/up_arrow" android:isRepeatable="true" android:horizontalGap="0.5%p" />
	</Row>
	<Row android:keyWidth="10%p">
		<Key android:codes="-102" android:keyIcon="@drawable/left_arrow"
			android:iconPreview="@drawable/left_arrow" android:keyEdgeFlags="right"
