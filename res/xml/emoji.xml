<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
	android:keyWidth="12.5%p" android:horizontalGap="0px"
	android:verticalGap="@dimen/vertical_gap" android:keyHeight="@dimen/emoji_key_height">

    <Row>
        <Key android:codes="0" android:keyLabel="#TAB#" android:keyEdgeFlags="left" />
        <Key android:codes="1" android:keyLabel="#TAB#" />
        <Key android:codes="2" android:keyLabel="#TAB#" />
        <Key android:codes="3" android:keyLabel="#TAB#" />
        <Key android:codes="4" android:keyLabel="#TAB#" />
        <Key android:codes="5" android:keyLabel="#TAB#" />
        <Key android:codes="6" android:keyLabel="#TAB#" />
        <Key android:codes="7" android:keyLabel="#TAB#" android:keyEdgeFlags="right" />
    </Row>

    <Row>
        <Key android:codes="0" android:keyLabel="#CODE#" bigText="true" android:keyEdgeFlags="left" />
        <Key android:codes="1" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="2" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="3" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="4" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="5" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="6" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="7" android:keyLabel="#CODE#" bigText="true" android:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key android:codes="8" android:keyLabel="#CODE#" bigText="true" android:keyEdgeFlags="left" />
        <Key android:codes="9" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="10" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="11" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="12" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="13" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="14" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="15" android:keyLabel="#CODE#" bigText="true" android:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key android:codes="16" android:keyLabel="#CODE#" bigText="true" android:keyEdgeFlags="left" />
        <Key android:codes="17" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="18" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="19" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="20" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="21" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="22" android:keyLabel="#CODE#" bigText="true" />
        <Key android:codes="23" android:keyLabel="#CODE#" bigText="true" android:keyEdgeFlags="right" />
    </Row>


    <Row android:rowEdgeFlags="bottom" android:keyHeight="@dimen/emoji_bottom_key_height">
        <Key android:codes="-116" android:keyLabel="ABC" android:keyWidth="15%p"
            android:keyEdgeFlags="left" />
        <Key android:codes="-113" android:horizontalGap="11%"
            android:isRepeatable="true" android:keyLabel="&#9664;" android:keyWidth="18%p" />
        <Key android:codes="-115" android:keyLabel="#EMPAGE#" android:horizontalGap="4%"
            android:isRepeatable="true" android:keyWidth="13%p" />
        <Key android:codes="-114" android:keyLabel="&#9654;" android:horizontalGap="4%" 
            android:isRepeatable="true" android:keyWidth="18%p" />
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete"
            android:horizontalGap="4%" android:iconPreview="@drawable/sym_keyboard_feedback_delete"
            iconID="@+id/delete_key" android:keyWidth="13%p"
            android:keyEdgeFlags="right" android:isRepeatable="true" />
    </Row>
</Keyboard>
