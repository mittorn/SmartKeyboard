<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
	android:keyWidth="16.7%p" android:horizontalGap="0px"
	android:verticalGap="@dimen/vertical_gap" android:keyHeight="@dimen/key_height">

	<Row>
		<Key android:codes="40" android:keyLabel="(" android:keyEdgeFlags="left" />
		<Key android:codes="49" android:keyLabel="1" altLabel=""/>
		<Key android:codes="50" android:keyLabel="2" altLabel="abc" android:popupCharacters="abc" />
		<Key android:codes="51" android:keyLabel="3" altLabel="def" android:popupCharacters="def" />
		<Key android:codes="46" android:keyLabel="." />
		<Key android:codes="44" android:keyWidth="16.5%p" android:keyLabel="P" android:keyEdgeFlags="right" />
	</Row>

	<Row>
		<Key android:codes="41" android:keyLabel=")" android:keyEdgeFlags="left" />
		<Key android:codes="52" android:keyLabel="4" altLabel="ghi" android:popupCharacters="ghi" />
		<Key android:codes="53" android:keyLabel="5" altLabel="jkl" android:popupCharacters="jkl" />
		<Key android:codes="54" android:keyLabel="6" altLabel="mno" android:popupCharacters="mno"/>
		<Key android:codes="44" android:keyLabel="," />
		<Key android:codes="59" android:keyWidth="16.5%p" android:keyLabel="W" android:keyEdgeFlags="right" />
	</Row>

	<Row>
		<Key android:codes="43" android:keyLabel="+" android:keyEdgeFlags="left" />
		<Key android:codes="55" android:keyLabel="7" altLabel="pqrs" android:popupCharacters="pqrs" />
		<Key android:codes="56" android:keyLabel="8" altLabel="tuv" android:popupCharacters="tuv" />
		<Key android:codes="57" android:keyLabel="9" altLabel="wxyz" android:popupCharacters="wxyz" />
		<Key android:codes="45" android:keyLabel="-" />
		<Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete"
			android:iconPreview="@drawable/sym_keyboard_feedback_delete" 
			iconID="@+id/delete_key" android:keyWidth="16.5%p" 
			android:keyEdgeFlags="right" android:isRepeatable="true" />
	</Row>

	<Row android:rowEdgeFlags="bottom">
		<Key android:codes="47" android:keyLabel="/" android:keyEdgeFlags="left" />
		<Key android:codes="42" android:keyLabel="*" />
		<Key android:codes="48" android:keyLabel="0" />
		<Key android:codes="35" android:keyLabel="\#" />
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
			android:iconPreview="@drawable/sym_keyboard_feedback_space" iconID="@+id/space_key"
			android:isRepeatable="false" />
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
			android:iconPreview="@drawable/sym_keyboard_feedback_return" iconID="@+id/return_key"
			android:keyWidth="16.5%p" android:keyEdgeFlags="right" />
	</Row>

</Keyboard>
