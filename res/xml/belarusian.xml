		<Key android:codes="1075" android:keyLabel="г" altLabel="7" android:popupCharacters="7"/>
		<Key android:codes="1096" android:keyLabel="ш" altLabel="8" android:popupCharacters="8"/>
		<Key android:codes="1118" android:keyLabel="ў" altLabel="9" android:popupCharacters="9"/>
		<Key android:codes="1079" android:keyLabel="з" altLabel="0" android:popupCharacters="0"/>
		<Key android:codes="1093" android:keyLabel="х"  altLabel="" android:keyWidth="8.8%p" android:keyEdgeFlags="right" />		
	</Row>
	<Row>
		<Key android:codes="1092" android:keyLabel="ф" altLabel="\@" android:popupCharacters="\@"
			android:keyEdgeFlags="left" />
		<Key android:codes="1099" android:keyLabel="ы" altLabel="\#" android:popupCharacters="\#"/>
