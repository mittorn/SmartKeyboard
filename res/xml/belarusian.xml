		<Key android:codes="1091" android:keyLabel="у" altLabel="3" android:popupCharacters="3"/>
		<Key android:codes="1082" android:keyLabel="к" altLabel="4" android:popupCharacters="4"/>
		<Key android:codes="1077" android:keyLabel="е" altLabel="5" android:popupCharacters="5ё"
