<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
	android:keyWidth="10%p" android:horizontalGap="0px"
	android:verticalGap="@dimen/vertical_gap" android:keyHeight="@dimen/key_height">

    <Include xml="@xml/numbers_top"/>
	<Row android:keyWidth="10%p">
    	<Key android:codes="1590" android:keyLabel="&#1590;" altLabel="۱" android:popupCharacters="۱" android:keyEdgeFlags="left"/>
        <Key android:codes="1589" android:keyLabel="&#1589;" altLabel="۲" android:popupCharacters="۲"/>
        <Key android:codes="1602" android:keyLabel="&#1602;" altLabel="۳" android:popupCharacters="۳"/>
        <Key android:codes="1601" android:keyLabel="&#1601;" altLabel="۴" android:popupCharacters="۴"/>
        <Key android:codes="1594" android:keyLabel="&#1594;" altLabel="۵" android:popupCharacters="۵"/>
        <Key android:codes="1593" android:keyLabel="&#1593;" altLabel="۶" android:popupCharacters="۶"/>
        <Key android:codes="1607" android:keyLabel="&#1607;" altLabel="۷" android:popupCharacters="۷"/>
        <Key android:codes="1582" android:keyLabel="&#1582;" altLabel="۸" android:popupCharacters="۸"/>
        <Key android:codes="1581" android:keyLabel="&#1581;" altLabel="۹" android:popupCharacters="۹"/>
        <Key android:codes="1580" android:keyLabel="&#1580;" altLabel="۰" android:popupCharacters="۰" 
