    </Row>
    <Row android:keyWidth="10%p">
        <Key android:codes="1588" android:keyLabel="&#1588;" altLabel="\@" android:popupCharacters="\@" android:keyEdgeFlags="left"/>
        <Key android:codes="1587" android:keyLabel="&#1587;" altLabel="\#" android:popupCharacters="\#"/>
        <Key android:codes="1740" android:keyLabel="&#1740;" altLabel="$" android:popupCharacters="$&#1574;"/>
        <Key android:codes="1576" android:keyLabel="&#1576;" altLabel="&amp;" android:popupCharacters="&amp;"/>
        <Key android:codes="1604" android:keyLabel="&#1604;" altLabel="*" android:popupCharacters="*"/>
        <Key android:codes="1575" android:keyLabel="&#1575;" altLabel="-" android:popupCharacters="&#1571;&#1573;&#1570;&#1569;-"/>
        <Key android:codes="1578" android:keyLabel="&#1578;" altLabel="+" android:popupCharacters="&#1579;+"/>
        <Key android:codes="1606" android:keyLabel="&#1606;" altLabel="(" android:popupCharacters="("/>
        <Key android:codes="1605" android:keyLabel="&#1605;" altLabel=")" android:popupCharacters=")"/>
        <Key android:codes="1705" android:keyLabel="&#1705;" altLabel="&#1567;" android:popupCharacters="&#1567;" android:keyEdgeFlags="right"/>
    </Row>
    <Row android:keyWidth="10%p">
    	<Key android:codes="1592" android:keyLabel="&#1592;" altLabel="!" android:popupCharacters="!" android:keyEdgeFlags="left"/>
    	<Key android:codes="1591" android:keyLabel="&#1591;" altLabel="&quot;" android:popupCharacters="&quot;"/>
    	<Key android:codes="1583" android:keyLabel="&#1583;" altLabel="'" android:popupCharacters="'&#1584;"/>
    	<Key android:codes="1586" android:keyLabel="&#1586;" altLabel=":" android:popupCharacters=":&#1688;"/>
    	<Key android:codes="1585" android:keyLabel="&#1585;" altLabel=";" android:popupCharacters=";"/>
    	<Key android:codes="1608" android:keyLabel="&#1608;" altLabel="/" android:popupCharacters="&#1572;/"/>
      	<Key android:codes="1670" android:keyLabel="&#1670;" altLabel="%" android:popupCharacters="%"/>
    	<Key android:codes="1662" android:keyLabel="&#1662;" altLabel="[" android:popupCharacters="["/>
        <Key android:codes="1711" android:keyLabel="&#1711;" altLabel="]" android:popupCharacters="]" android:keyEdgeFlags="right"/>
		<Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" 
			android:iconPreview="@drawable/sym_keyboard_feedback_delete" 
			iconID="@+id/delete_key" android:keyEdgeFlags="right" android:isRepeatable="true" />
    </Row>
	<Row android:rowEdgeFlags="bottom" android:keyboardMode="@+id/mode_normal">
		<Key android:codes="-2" android:keyLabel="\?123" android:keyWidth="15%p" android:keyEdgeFlags="left" />
		<Key android:codes="1548" android:keyLabel="&#1548;" android:keyWidth="10%p"/>
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
			android:iconPreview="@drawable/sym_keyboard_feedback_space" iconID="@+id/space_key"
			android:keyWidth="47%p" android:isRepeatable="false" />
		<Key android:codes="46" android:keyLabel="." android:keyWidth="10%p"/>
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
			android:iconPreview="@drawable/sym_keyboard_feedback_return" iconID="@+id/return_key"
			android:keyWidth="18%p" android:keyEdgeFlags="right" />
	</Row>
	<Row android:rowEdgeFlags="bottom" android:keyboardMode="@+id/mode_lang">
		<Key android:codes="-2" android:keyLabel="123" android:keyWidth="12%p" android:keyEdgeFlags="left" />
		<Key android:codes="-101" android:keyLabel="" android:keyWidth="10%p"/>
		<Key android:codes="1548" android:keyLabel="&#1548;" android:keyWidth="10%p" />
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
			android:iconPreview="@drawable/sym_keyboard_feedback_space" iconID="@+id/space_key"
			android:keyWidth="40%p" android:isRepeatable="false" />
		<Key android:codes="46" android:keyLabel="." android:keyWidth="10%p" />
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
			android:iconPreview="@drawable/sym_keyboard_feedback_return" iconID="@+id/return_key"
			android:keyWidth="18%p" android:keyEdgeFlags="right" />
	</Row>
	<Row android:rowEdgeFlags="bottom" android:keyboardMode="@+id/mode_url">
		<Key android:codes="-2" android:keyLabel="123" android:keyWidth="12%p" android:keyEdgeFlags="left" />
		<Key android:keyLabel="%domain_00" android:keyOutputText="%domain_00"
 			android:popupKeyboard="@xml/popup_domains" android:keyWidth="15%p" />
		<Key android:codes="47" android:keyLabel="/" android:keyWidth="12%p" />
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
			android:iconPreview="@drawable/sym_keyboard_feedback_space" iconID="@+id/space_key"
			android:keyWidth="33%p" android:isRepeatable="false" />
		<Key android:codes="46" android:keyLabel="."  altLabel="&#1548;"
			android:popupCharacters="&#1548;" android:keyWidth="10%p" />
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
			android:iconPreview="@drawable/sym_keyboard_feedback_return" iconID="@+id/return_key"
			android:keyWidth="18%p" android:keyEdgeFlags="right" />
	</Row>
	<Row android:rowEdgeFlags="bottom" android:keyboardMode="@+id/mode_lang_url">
		<Key android:codes="-2" android:keyLabel="123" android:keyWidth="12%p" android:keyEdgeFlags="left" />
		<Key android:codes="-101" android:keyLabel="" android:keyWidth="10%p"/>
		<Key android:keyLabel="%domain_00" android:keyOutputText="%domain_00"
 			android:popupKeyboard="@xml/popup_domains" android:keyWidth="15%p" />
		<Key android:codes="47" android:keyLabel="/" android:keyWidth="12%p" />
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
			android:iconPreview="@drawable/sym_keyboard_feedback_space" iconID="@+id/space_key"
			android:keyWidth="23%p" android:isRepeatable="false" />
		<Key android:codes="46" android:keyLabel="."  altLabel="&#1548;"
			android:popupCharacters="&#1548;" android:keyWidth="10%p" />
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
			android:iconPreview="@drawable/sym_keyboard_feedback_return" iconID="@+id/return_key"
			android:keyWidth="18%p" android:keyEdgeFlags="right" />
	</Row>
</Keyboard>
