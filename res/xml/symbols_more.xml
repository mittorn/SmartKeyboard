<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
	android:keyWidth="10%p" android:horizontalGap="0px"
	android:verticalGap="@dimen/vertical_gap" android:keyHeight="@dimen/key_height">

	<Row>
		<Key android:codes="49" android:keyLabel="1" altLabel="~" android:popupCharacters="~¹" android:keyEdgeFlags="left" />
		<Key android:codes="50" android:keyLabel="2" altLabel="±" android:popupCharacters="±²½" />
		<Key android:codes="51" android:keyLabel="3" altLabel="×" android:popupCharacters="×π³⅓⅔" />
		<Key android:codes="52" android:keyLabel="4" altLabel="÷" android:popupCharacters="÷¼¾" />
		<Key android:codes="53" android:keyLabel="5" altLabel="•" android:popupCharacters="•⅕" />
		<Key android:codes="54" android:keyLabel="6" altLabel="°" android:popupCharacters="⅚⅙°" />
		<Key android:codes="55" android:keyLabel="7" altLabel="`" android:popupCharacters="⅐`" />
		<Key android:codes="56" android:keyLabel="8" altLabel="´" android:popupCharacters="∞⅞⅝⅜⅛´" />
		<Key android:codes="57" android:keyLabel="9" altLabel="{" android:popupCharacters="⅑{" />
		<Key android:codes="48" android:keyLabel="0" altLabel="}" android:popupCharacters="º∅}"  android:keyEdgeFlags="right" />
	</Row>

	<Row>
		<Key android:codes="64" android:keyLabel="\@" altLabel="©" android:popupCharacters="©" 
			android:keyEdgeFlags="left" />
		<Key android:codes="35" android:keyLabel="\#" altLabel="£" android:popupCharacters="£" />
		<Key android:codes="36" android:keyLabel="$" altLabel="€" android:popupCharacters="€" />
		<Key android:codes="37" android:keyLabel="%" altLabel="^" android:popupCharacters="^‰" />
		<Key android:codes="38" android:keyLabel="&amp;" altLabel="®" android:popupCharacters="®" />
		<Key android:codes="42" android:keyLabel="*" altLabel="¥" android:popupCharacters="∏¥" />
		<Key android:codes="45" android:keyLabel="-" altLabel="_" android:popupCharacters="—–_" />
		<Key android:codes="43" android:keyLabel="+" altLabel="=" android:popupCharacters="∑=" />
		<Key android:codes="40" android:keyLabel="(" altLabel="[" android:popupCharacters="[" />
		<Key android:codes="41" android:keyLabel=")" altLabel="]" android:popupCharacters="]"  android:keyEdgeFlags="right" />
	</Row>

	<Row android:keyWidth="10.2%p">
		<Key android:codes="-1" android:keyLabel="1/2" android:keyWidth="12.6%p" android:keyEdgeFlags="left" />
		<Key android:codes="33" android:keyLabel="!" altLabel="¡" android:popupCharacters="¡"  android:horizontalGap="1.4%"/>
		<Key android:codes="34" android:keyLabel="&quot;" altLabel="&lt;" android:popupCharacters="&lt;„“«»”" />
		<Key android:codes="39" android:keyLabel="\'" altLabel="&gt;" android:popupCharacters="&gt;" />
		<Key android:codes="58" android:keyLabel=":" altLabel="¢" android:popupCharacters="¢" />
		<Key android:codes="59" android:keyLabel=";" altLabel="|" android:popupCharacters="|" />
		<Key android:codes="47" android:keyLabel="/" altLabel="\\" android:popupCharacters="\\" />
		<Key android:codes="63" android:keyLabel="\?" altLabel="¿" android:popupCharacters="‽¿" />
		<Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete"
			android:iconPreview="@drawable/sym_keyboard_feedback_delete" android:horizontalGap="1.6%"
			iconID="@+id/delete_key" android:keyWidth="13%p"
			android:keyEdgeFlags="right" android:isRepeatable="true" />
	</Row>

	<Row android:rowEdgeFlags="bottom" android:keyboardMode="@+id/mode_normal">
		<Key android:codes="-2" android:keyLabel="ABC" android:keyWidth="15%p" android:keyEdgeFlags="left" />
		<Key android:codes="44" android:keyLabel="," android:keyWidth="10%p"/>
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
			android:iconPreview="@drawable/sym_keyboard_feedback_space" iconID="@+id/space_key"
			android:keyWidth="47%p" android:isRepeatable="false" />
		<Key android:codes="46" android:keyLabel="." android:keyWidth="10%p"/>
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
			android:iconPreview="@drawable/sym_keyboard_feedback_return" iconID="@+id/return_key"
			android:keyWidth="18%p" android:keyEdgeFlags="right" />
	</Row>
	
		<Row android:rowEdgeFlags="bottom" android:keyboardMode="@+id/mode_arrows">
		<Key android:codes="-107" android:keyLabel="&lt; &gt;" android:keyWidth="12%p" android:keyEdgeFlags="left" />
		<Key android:codes="-2" android:keyLabel="ABC" android:keyWidth="12%p" />
		<Key android:codes="44" android:keyLabel="," android:keyWidth="10%p"/>
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
			android:iconPreview="@drawable/sym_keyboard_feedback_space" iconID="@+id/space_key"
			android:keyWidth="38%p" android:isRepeatable="false" />
		<Key android:codes="46" android:keyLabel="." android:keyWidth="10%p"/>
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
			android:iconPreview="@drawable/sym_keyboard_feedback_return" iconID="@+id/return_key"
			android:keyWidth="18%p" android:keyEdgeFlags="right" />
	</Row>
</Keyboard>
