<Rows xmlns:android="http://schemas.android.com/apk/res/android">
	<Row android:rowEdgeFlags="bottom" android:keyboardMode="@+id/mode_normal">
		<Key android:codes="-2" android:keyLabel="123" android:keyWidth="11%p" 
			android:keyEdgeFlags="left"/>
		<Key android:codes="-109" android:keyLabel="XT9" android:isModifier="true"
		 	android:horizontalGap="0.5%p" android:isSticky="true" android:keyWidth="10%p" />
		<Key android:codes="-108" android:iconPreview="@drawable/mic_black" android:keyWidth="10%p" 
			android:horizontalGap="0.5%p" commaWidth="0.0" />
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space"
			android:iconPreview="@drawable/sym_keyboard_feedback_space" iconID="@+id/space_key"
			android:keyWidth="44.5%p" android:isRepeatable="false" commaWidth="10.0" periodWidth="10.0" />
		<Key android:codes="46" android:keyLabel="." altLabel=","
			android:popupCharacters="," android:keyWidth="10%p" periodWidth="0.0" />
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
			android:iconPreview="@drawable/sym_keyboard_feedback_return" iconID="@+id/return_key"
			android:horizontalGap="0.5%p" android:keyWidth="13%p" android:keyEdgeFlags="right" />
	</Row>
	<Row android:rowEdgeFlags="bottom" android:keyboardMode="@+id/mode_lang">
		<Key android:codes="-2" android:keyLabel="123" android:keyWidth="11%p" 
			android:keyEdgeFlags="left"/>
		<Key android:codes="-101" android:keyLabel="" android:keyWidth="10%p" />
		<Key android:codes="-108" android:iconPreview="@drawable/mic_black" android:keyWidth="10%p"
			commaWidth="0.0" />
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space"
			android:iconPreview="@drawable/sym_keyboard_feedback_space" iconID="@+id/space_key"
			android:keyWidth="35%p" android:isRepeatable="false" commaWidth="10.0" periodWidth="10.0" />
		<Key android:codes="46" android:keyLabel="." altLabel="," periodWidth="0.0"
			android:popupCharacters="," android:keyWidth="10%p"/>
		<Key android:codes="-109" android:keyLabel="XT9" android:isModifier="true"
		 	android:horizontalGap="0.5%p" android:isSticky="true" android:keyWidth="10%p" />
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
			android:iconPreview="@drawable/sym_keyboard_feedback_return" iconID="@+id/return_key"
			android:horizontalGap="0.5%p" android:keyWidth="13%p" android:keyEdgeFlags="right" />
	</Row>
	<Row android:rowEdgeFlags="bottom" android:keyboardMode="@+id/mode_url">
		<Key android:codes="-2" android:keyLabel="123" android:keyWidth="11%p" 
			android:keyEdgeFlags="left"/>
		<Key android:keyLabel="%domain_00" android:keyOutputText="%domain_00"
			android:popupKeyboard="@xml/popup_domains" android:keyWidth="15%p" />
		<Key android:codes="47" android:keyLabel="/" android:keyWidth="11%p" />
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space"
			android:iconPreview="@drawable/sym_keyboard_feedback_space" iconID="@+id/space_key"
			android:keyWidth="29%p" android:isRepeatable="false" />
		<Key android:codes="46" android:keyLabel="." altLabel=","
			android:popupCharacters="," android:keyWidth="10%p" />
		<Key android:codes="-109" android:keyLabel="XT9" android:isModifier="true"
		 	android:horizontalGap="0.5%p" android:isSticky="true" android:keyWidth="10%p" />
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
			android:iconPreview="@drawable/sym_keyboard_feedback_return" iconID="@+id/return_key"
			android:horizontalGap="0.5%p" android:keyWidth="13%p" android:keyEdgeFlags="right" />
	</Row>
	<Row android:rowEdgeFlags="bottom" android:keyboardMode="@+id/mode_lang_url">
		<Key android:codes="-2" android:keyLabel="123" android:keyWidth="10%p" 
			android:keyEdgeFlags="left"/>
		<Key android:codes="-101" android:keyLabel="" android:keyWidth="10%p" />
		<Key android:keyLabel="%domain_00"
			android:keyOutputText="%domain_00"
			android:popupKeyboard="@xml/popup_domains" android:keyWidth="14%p" />
		<Key android:codes="47" android:keyLabel="/" android:keyWidth="12%p" />
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space"
			android:iconPreview="@drawable/sym_keyboard_feedback_space" iconID="@+id/space_key"
			android:keyWidth="20%p" android:isRepeatable="false" />
		<Key android:codes="46" android:keyLabel="." altLabel=","
			android:popupCharacters="," android:keyWidth="10%p" />
		<Key android:codes="-109" android:keyLabel="XT9" android:isModifier="true"
		 	android:horizontalGap="0.5%p" android:isSticky="true" android:keyWidth="10%p" />
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
			android:iconPreview="@drawable/sym_keyboard_feedback_return" iconID="@+id/return_key"
			android:horizontalGap="0.5%p" android:keyWidth="13%p" android:keyEdgeFlags="right" />
	</Row>
</Rows>