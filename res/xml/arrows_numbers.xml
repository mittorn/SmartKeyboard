<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
	android:keyWidth="15%p" android:horizontalGap="0px"
	android:verticalGap="@dimen/vertical_gap" android:keyHeight="@dimen/key_height">
	<Row>
		<Key android:codes="27" android:keyLabel="Esc"
			android:keyWidth="16%p" android:keyEdgeFlags="left"  />
		<Key android:codes="-104" android:keyIcon="@drawable/up_arrow"
			android:iconPreview="@drawable/up_arrow" android:keyWidth="16%p"
			iconID="@+id/up_arrow" android:isRepeatable="true" />
		<Key android:codes="49" android:keyWidth="18%p" android:keyLabel="1" altLabel=""/>
		<Key android:codes="50" android:keyWidth="18%p" android:keyLabel="2" altLabel=""/>
		<Key android:codes="51" android:keyWidth="18%p" android:keyLabel="3" altLabel=""/>
		<Key android:codes="45" android:keyWidth="14%p" android:keyLabel="-" altLabel="/"
			android:popupCharacters="/" android:keyEdgeFlags="right"/>
	</Row>

	<Row>
		<Key android:codes="-102" android:keyIcon="@drawable/left_arrow"
			android:iconPreview="@drawable/left_arrow" android:keyWidth="16%p"
			android:isRepeatable="true" iconID="@+id/left_arrow"
			android:keyEdgeFlags="left" />
		<Key android:codes="-103" android:keyIcon="@drawable/right_arrow"
			android:isRepeatable="true" android:iconPreview="@drawable/right_arrow"
		 	android:keyWidth="16%p" iconID="@+id/right_arrow" />
		<Key android:codes="52" android:keyWidth="18%p" android:keyLabel="4" altLabel=""/>
		<Key android:codes="53" android:keyWidth="18%p" android:keyLabel="5" altLabel=""/>
		<Key android:codes="54" android:keyWidth="18%p" android:keyLabel="6" altLabel=""/>
		<Key android:codes="43" android:keyWidth="14%p" android:keyLabel="+" altLabel="*"
			android:popupCharacters="*" android:keyEdgeFlags="right"/>
	</Row>

	<Row>
		<Key android:codes="-106" android:keyLabel="Tab" android:keyWidth="16%p"
			android:keyEdgeFlags="left" />
		<Key android:codes="-105" android:keyIcon="@drawable/down_arrow"
			android:iconPreview="@drawable/down_arrow" android:isRepeatable="true"
			android:keyWidth="16%p" iconID="@+id/down_arrow" />
		<Key android:codes="55" android:keyWidth="18%p" android:keyLabel="7" altLabel=""/>
		<Key android:codes="56" android:keyWidth="18%p" android:keyLabel="8" altLabel=""/>
		<Key android:codes="57" android:keyWidth="18%p" android:keyLabel="9" altLabel=""/>
		<Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete"
			android:iconPreview="@drawable/sym_keyboard_feedback_delete"
			android:keyWidth="14%p" iconID="@+id/delete_key"
			android:keyEdgeFlags="right" android:isRepeatable="true" />
	</Row>

	<Row android:rowEdgeFlags="bottom">
		<Key android:codes="-2" android:keyLabel="ABC" android:keyWidth="15%p"
			android:keyEdgeFlags="left" />
		<Key android:codes="44" android:keyLabel="," android:keyWidth="14%p" />
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space"
			android:iconPreview="@drawable/sym_keyboard_feedback_space" iconID="@+id/space_key"
			android:keyWidth="21%p" android:isRepeatable="false" />
		<Key android:codes="48" android:keyWidth="18%p" android:keyLabel="0" altLabel=""/>
		<Key android:codes="46" android:keyLabel="." android:keyWidth="14%p" />
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
			android:iconPreview="@drawable/sym_keyboard_feedback_return" iconID="@+id/return_key"
			android:keyWidth="18%p" alwaysEnter="true" android:keyEdgeFlags="right" />
	</Row>
</Keyboard>
