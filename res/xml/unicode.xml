<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
	android:keyWidth="20%p" android:horizontalGap="0px"
	android:verticalGap="@dimen/vertical_gap" android:keyHeight="@dimen/key_height">

	<Row>
		<Key android:codes="48" android:keyLabel="0" android:keyEdgeFlags="left" />
		<Key android:codes="49" android:keyLabel="1" />
		<Key android:codes="50" android:keyLabel="2" />
		<Key android:codes="51" android:keyLabel="3" />
		<Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete"
			android:iconPreview="@drawable/sym_keyboard_feedback_delete" 
			iconID="@+id/delete_key" android:keyEdgeFlags="right" android:isRepeatable="true" />
	</Row>
	<Row>
		<Key android:codes="52" android:keyLabel="4" android:keyEdgeFlags="left" />
		<Key android:codes="53" android:keyLabel="5" />
		<Key android:codes="54" android:keyLabel="6" />
		<Key android:codes="55" android:keyLabel="7" />
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
			android:iconPreview="@drawable/sym_keyboard_feedback_space" iconID="@+id/space_key"
			android:isRepeatable="false" android:keyEdgeFlags="right" />
	</Row>

	<Row>
		<Key android:codes="56" android:keyLabel="8" android:keyEdgeFlags="left" />
		<Key android:codes="57" android:keyLabel="9" />
		<Key android:codes="65" android:keyLabel="A" />
		<Key android:codes="66" android:keyLabel="B" />
		<Key android:codes="-2" android:keyLabel="ABC" android:keyEdgeFlags="right" />
	</Row>

	<Row android:rowEdgeFlags="bottom">
		<Key android:codes="67" android:keyLabel="C" android:keyEdgeFlags="left" />
		<Key android:codes="68" android:keyLabel="D" />
		<Key android:codes="69" android:keyLabel="E" />
		<Key android:codes="70" android:keyLabel="F" />
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
			android:iconPreview="@drawable/sym_keyboard_feedback_return" iconID="@+id/return_key"
			android:keyEdgeFlags="right" />
	</Row>

</Keyboard>
